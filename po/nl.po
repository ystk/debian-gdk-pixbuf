#, c-format
msgid "Failed to open file '%s': %s"
msgstr "Openen van bestand ‘%s’ mislukt: %s"

#: gdk-pixbuf/gdk-pixbuf-animation.c:171 gdk-pixbuf/gdk-pixbuf-io.c:962
#, c-format
msgid "Image file '%s' contains no data"
